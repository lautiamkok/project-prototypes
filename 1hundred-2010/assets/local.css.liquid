/* @group slide */

div#slide{
	margin:0px 0px 8px 0px;
	position:relative;
	border:0px solid #fff;
	/** float:left;  a must for IE **/
	}
	
div#slide, 
div#slide li {
	width:784px;
	height:371px;
	overflow:hidden;
	}
	
/*slideshow style*/	
ul.slide {
	width:784px;
	height:371px;
	overflow:hidden;
	clear:both;
	}
	
ul.slide li {
	position:absolute;
	z-index:0;
	display:none;
	}
	
ul.slide li:first-child {
	display:block;
	}
	
ul.slide li.active {
    z-index:10;
	display:block;
	}

ul.slide li.last-active {
    z-index:9;
	display:block;
	}

/* @end */


