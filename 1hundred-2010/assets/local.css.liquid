a.active,
a.current {
	color:#a7a7a7 !important;
	}
	
.hide {
	display:none;
	}
	
.hide-text {
	text-indent: -3000px; /* sends the text off-screen */
	outline: none; /* if you have any links in elements with the negative text-indent, make sure you also specify "outline: none" otherwise you get a dotted border going off the left of screen */
	 white-space: nowrap; /* add 'white-space: nowrap' just to be on the safe side if your text is long as only the first line is indented.*/
	 text-align: left;
	}
	
.left {
	float:left !important;
	}
	
.right {
	float:right !important;
	}
	
.font-baskerville {
	font-weight: normal;
	font-style: normal;
	line-height:normal;
	font-family: 'ITCNewBaskervilleStdItalic', sans-serif;
	}

.font-chunkfiveroman {
	font-weight: normal;
	font-style: normal;
	line-height:normal;
	font-family: 'ChunkFiveRoman', sans-serif;
	font-size-adjust: 0.54;
	}

.round-corner-2px {
	border-radius: 2px;
	-moz-border-radius: 2px;
	-webkit-border-radius: 2px;
	}
	
.round-corner-3px {
	border-radius: 3px;
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	}
	
.round-corner-10px {
	border-radius: 10px;
	-moz-border-radius: 10px;
	-webkit-border-radius: 10px;
	}
	
.hover-opacity-04:hover{
	opacity:0.4 !important;
	-ms-filter:"progid:DXImageTransform.Microsoft.Alpha(Opacity=40)"; /** for IE8 **/
	filter:alpha(opacity=40) !important; /** for IE5-7 **/
	}

div#container {
	text-align: left;
	width: 1025px;
	margin: 0 auto;
	}

	
div#container div{
	clear:both;
	overflow:hidden;
	border:0px solid #0066FF;
	}
	
div#container, div#header, div#index, div#collection, div#descriptions, div#product, div#product-select, div#navigate-collection, div#cart, div.cart-row, div.updater, div#checkout, div#blog, div#page {
	display: inline-block;
	}

div#container, div#header, div#index, div#collection, div#descriptions, div#product, div#product-select, div#navigate-collection, div#cart, div.cart-row, div.updater, div#checkout, div#blog, div#page {
	display: block;
	}

div#container:after, div#header:after, div#index:after, div#collection:after, div#descriptions:after, div#product:after, div#product-select:after, div#navigate-collection:after, div#cart:after, div.cart-row:after, div.updater:after, div#checkout:after, div#blog:after, div#page:after {
	content: ".";
	display: block;
	height: 0;
	clear: both;
	visibility: hidden;
	}

